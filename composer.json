{
    "name": "darwinnatha/purge-logs",
    "description": "A simple package to purge logs from a period",
    "type": "library",
    "authors": [
        {
            "name": "Darwin Nathan",
            "email": "fotsodarwin@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Darwinnatha\\PurgeLogs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Darwinnatha\\PurgeLogs\\PurgeLogsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}